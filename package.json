{
  "name": "config_file.js",
  "version": "1.4.1",
  "description": "A config file management library for Node.js.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "npx eslint . --config .eslintrc.json --ext .js,.jsx,.cjs,.mjs,.ts,.tsx --ignore-path .eslintignore",
    "lint:fix": "npx eslint . --config .eslintrc.json --ext .js,.jsx,.cjs,.mjs,.ts,.tsx --ignore-path .eslintignore --fix",
    "build": "node scripts/clean.js && npx tsc",
    "build:debug": "node scripts/clean.js && npx tsc --sourceMap",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "config",
    "json"
  ],
  "author": "yuko1101 (https://github.com/yuko1101)",
  "license": "MIT",
  "homepage": "https://github.com/yuko1101/ConfigFile.js#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuko1101/ConfigFile.js"
  },
  "bugs": {
    "url": "https://github.com/yuko1101/ConfigFile.js/issues"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
